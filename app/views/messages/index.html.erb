<div class="chatroom">
	<% @messages.each do |m| %>
		<div class="message">
			<h4><%= m.from_user %></h4>
			<p><%= m.text %></p>
		</div>
	<% end %>
	<div class="message_form">
		<%= simple_form_for @message do |f| %>
			<%= f.input :from_user %>
			<%= f.input :text %>
			<%= f.button :submit %>
		<% end %>
	</div>
</div>

